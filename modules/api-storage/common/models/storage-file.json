{
  "name": "StorageFile",
  "description": "Storage: Manage Files in a Domain",
  "base": "SystemBaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SystemDomain": {
      "foreignKey": "container",
      "required": false
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "created": {
      "type": "Date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "Date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "events": {
      "type": "hasMany",
      "model": "ContentEvent"
    },
    "pages": {
      "type": "hasMany",
      "model": "ContentPage"
    },
    "posts": {
      "type": "hasMany",
      "model": "ContentPost"
    },
    "products": {
      "type": "hasMany",
      "model": "ContentProduct"
    }
  },
  "acls": [],
  "methods": {}
}
