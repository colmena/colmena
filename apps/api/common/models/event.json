{
  "name": "Event",
  "description": "Content model for managing Events in a Domain",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "String",
      "required": true
    },
    "domainId": {
      "type": "String"
    },
    "fileId": {
      "type": "String"
    },
    "name": {
      "type": "String",
      "required": true
    },
    "date": {
      "type": "Date"
    },
    "location": {
      "type": "String"
    }
  },
  "validations": [],
  "relations": {
    "domain": {
      "type": "belongsTo",
      "model": "Domain"
    },
    "file": {
      "type": "belongsTo",
      "model": "File"
    }
  },
  "acls": [],
  "methods": {}
}
